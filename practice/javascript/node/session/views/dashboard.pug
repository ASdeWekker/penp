extends layout

block title
    -var title = "Dashboard";

block link
    -var selected = "DASHBOARD";

block content
    h1 Dashboard!
    p Welcome to your dashboard, you are now logged in!
    br
    h2 User information
    p First Name: #{user.firstName}
    p Last Name: #{user.lastName}
    p Email: #{user.email}
    p Data: #{user.data}
    a(href="/logout") Sign out
